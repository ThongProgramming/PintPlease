<div class="container">
  <table class="table">

    <% @data.each do |t| %>
        <tr>
          <td><%= t['author']['name'] %></td>
          <td><%= t['beer']['name'] %></td>
          <td><%= t['beer']['brewery'] %></td>
          <td><%= t['rating'] %></td>
          <td><%= t['likeCount'] %></td>
        </tr>

    <% end %>
  </table>
</div>